<head>
<style>
table {
    font-family: arial, sans-serif;
    border-collapse: collapse;
    width: 100%;
}

td, th {
    border: 1px solid #dddddd;
    text-align: left;
    padding: 8px;
}
</style>
</head>

<h2>Updated results:</h2>
<table>
  <tr>
    <th>Row ID</th>
    <th>Product</th>
    <th>Customer</th>
    <th>Measure</th>
    <th>Value</th>
    <th>Valid From</th>
    <th>Valid To</th>
  </tr>
  <% @data_sets.each do |set| %>
    <tr>
      <td><%= set.id %></td>
      <td><%= set.product %></td>
      <td><%= set.customer %></td>
      <td><%= set.measure %></td>
      <td><%= set.value %></td>
      <td><%= set.valid_from %></td>
      <td><%= set.valid_to %></td>
    </tr>
  <% end %>
</table>

<form action="/" method="get">
<input type="submit" value="Home">
</form>
<form action="/search" method="get">
<input type="submit" value="New Search">
</form>
